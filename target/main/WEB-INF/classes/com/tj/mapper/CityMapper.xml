<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
 PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
 "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="com.tj.mapper.CityMapper">

	<select id="getByProvId" parameterType="int" resultType="city">
		select * from city where pid=#{pid}
	</select>
	
	<select id="findByCid" parameterType="int" resultType="city">
		select * from city where cid=#{cid}
	</select>
</mapper>